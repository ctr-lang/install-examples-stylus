{
  "name": "ctr-brunch-install",
  "version": "0.4.0",
  "description": "Brunch with ctr; the best breakfast!",
  "license": "MIT",
  "author": "te schultz",
  "homepage": "https://ctr-lang.com",
  "keywords": [
    "boilerplate",
    "brunch",
    "ctr",
    "setup"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/artisin/ctr-install/tree/master/brunch"
  },
  "bugs": {
    "url": "https://github.com/artisin/ctr-install/issues"
  },
  "scripts": {
    "build": "brunch build --production",
    "build:test": "brunch build",
    "dev": "brunch watch --server",
    "setup": "yarn install --ignore-engines",
    "test": "yarn run build:test && mocha __tests__/ --colors --bail",
    "update": "ncu --upgradeAll --packageFile package.json",
    "update:check": "ncu --packageFile package.json",
    "update:install": "yarn run update && yarn run setup"
  },
  "devDependencies": {
    "auto-reload-brunch": "2.7.1",
    "autoprefixer-stylus": "0.13.0",
    "babel-brunch": "6.1.1",
    "babel-preset-es2015": "6.24.1",
    "brunch": "2.10.9",
    "clean-css-brunch": "2.10.0",
    "css-brunch": "2.10.0",
    "ctr": "0.4.0",
    "javascript-brunch": "2.10.0",
    "mocha": "3.3.0",
    "npm-check-updates": "2.11.1",
    "should": "11.2.1",
    "stylus-brunch": "2.10.0",
    "uglify-js-brunch": "2.10.0"
  }
}
