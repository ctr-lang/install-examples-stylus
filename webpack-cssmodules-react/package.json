{
  "name": "ctr-install-webpack",
  "version": "0.4.0",
  "description": "Example with webpack, react, css-modules, stylus, ctr",
  "license": "MIT",
  "author": "te schultz",
  "homepage": "https://ctr-lang.com",
  "keywords": [
    "boilerplate",
    "css-modules",
    "ctr",
    "react",
    "setup",
    "webpack"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/artisin/ctr-install/tree/master/webpack-cssmodules-react"
  },
  "bugs": {
    "url": "https://github.com/artisin/ctr-install/issues"
  },
  "scripts": {
    "build": "webpack -p",
    "build:test": "webpack",
    "dev": "webpack-dev-server --progress --colors",
    "setup": "yarn install --ignore-engines",
    "test": "yarn run build:test && mocha __tests__/ --colors --bail",
    "update": "ncu --upgradeAll --packageFile package.json",
    "update:check": "ncu --packageFile package.json",
    "update:install": "yarn run update && yarn run setup"
  },
  "dependencies": {
    "react": "15.5.4",
    "react-css-modules": "4.2.0",
    "react-dom": "15.5.4"
  },
  "devDependencies": {
    "autoprefixer": "7.0.1",
    "babel-core": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "browser-sync": "2.18.8",
    "browser-sync-webpack-plugin": "1.1.4",
    "clean-webpack-plugin": "0.1.16",
    "css-loader": "0.28.1",
    "ctr": "0.4.0",
    "ctrclass": "0.0.2",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "html-webpack-plugin": "2.28.0",
    "image-webpack-loader": "3.3.1",
    "mocha": "3.3.0",
    "npm-check-updates": "2.11.1",
    "postcss-loader": "2.0.5",
    "raw-loader": "0.5.1",
    "should": "11.2.1",
    "style-loader": "0.17.0",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.1",
    "webpack": "2.5.1",
    "webpack-dev-server": "2.4.5"
  }
